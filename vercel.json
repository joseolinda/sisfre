{
    "version": 2,
    "builds": [
        { "src": "/api/index.php", "use": "vercel-php@0.6.0" },
        { "src": "/html/**", "use": "@vercel/static" }
    ],
    "routes": [
        {
            "src": "/(css|js)/(.*)",
            "dest": "html/$1/$2"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_NAME": "Vercel Laravel",
        "APP_ENV": "local",
        "APP_DEBUG": "true",
        "APP_KEY": "base64:BkZCMJC02JhBG1+w9zyEfqkkNSS5Zke0ILw0tr7chq0=",
        "APP_URL": "https://sisfre.vercel.app",
        "VERCEL_DEMO_MODE": "true",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "array",
        "VIEW_COMPILED_PATH": "/storage/framework/views",
        "SSR_TEMP_PATH": "/tmp/ssr",
        "NODE_PATH": "node"
    }, 
    "buildCommand": "php composer.phar install --no-dev --no-interaction --no-ansi --no-progress --no-suggest --no-scripts --no-plugins --no-custom-installers --ignore-platform-reqs && php artisan config:cache && php artisan route:cache && php artisan view:cache"
}